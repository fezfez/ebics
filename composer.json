{
  "name": "fezfez/ebics",
  "type": "library",
  "description": "PHP library to communicate with bank through EBICS protocol.",
  "keywords": [
    "ebics",
    "PHP",
    "client",
    "symfony",
    "openssl",
    "rsa",
    "x509"
  ],
  "authors": [
    {
      "name": "StÃ©phane Demonchaux"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4 || ^8.0",
    "ext-dom": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
    "ext-json": "*",
    "phpseclib/phpseclib": ">=2.0.27",
    "symfony/http-client": "^5.1",
    "thecodingmachine/safe": "^1.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.58",
    "phpunit/phpunit": "^9.5",
    "infection/infection": "^0.20",
    "doctrine/coding-standard": "^8.2",
    "slevomat/coding-standard" : "^6.4.1",
    "seromenho/xml-validator": "^1.0",
    "ocramius/package-versions": "^1.9.0 || ^2.0.0",
    "vimeo/psalm": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Fezfez\\Ebics\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fezfez\\Ebics\\Tests\\": "tests"
    }
  },
  "suggest": {
    "silarhi/cfonb-parser": "If you need to parse CFONB datas from French Bank",
    "andrew-svirin/mt942-php": "If you need to parse format MT942 from VMK, STA requests."
  }
}
